<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension point="org.eclipse.sirius.componentization">
    <component class="e4sm.de.metamodel.design.Activator"
               id="e4sm.de.metamodel.design"
	       name="sirius">
    </component>
  </extension>
  <extension
        id="CustomIDE"
        point="org.eclipse.core.runtime.products">
     <product
           application="org.eclipse.ui.ide.workbench"
           name="E4SM IDE">
        <property
              name="appName"
              value="E4SM IDE">
        </property>
        <property
              name="windowImages"
              value="platform:/plugin/e4sm.de.metamodel.branding/icons/logo_16.gif,platform:/plugin/e4sm.de.metamodel.branding/icons/logo_32.gif,platform:/plugin/e4sm.de.metamodel.branding/icons/logo_64.gif">
        </property>
        <property
              name="aboutImage"
              value="platform:/plugin/e4sm.de.metamodel.branding/icons/logo_64.gif">
        </property>
        <property
              name="preferenceCustomization"
              value="platform:/plugin/e4sm.de.metamodel.design/plugin_customization.ini">
        </property>
        <property
              name="aboutText"
              value="Welcome to the E4SM Editor&apos;s About Page&#x0A;Here you can .....">
        </property>
     </product>
  </extension>
</plugin>
