<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension point="org.eclipse.sirius.componentization">
    <component class="abcd.xy.metamodel.design.Activator"
               id="abcd.xy.metamodel.design"
	       name="sirius">
    </component>
  </extension>
  <extension
        point="org.eclipse.sirius.externalJavaAction">
     <javaActions
           actionClass="abcd.xy.metamodel.design.OpenEmbeddedEditor"
           id="abcd.xy.metamodel.design.OpenEmbeddedEditor">
     </javaActions>
  </extension>
  <extension
        id="CustomIDE"
        point="org.eclipse.core.runtime.products">
     <product
           application="org.eclipse.ui.ide.workbench"
           name="ABCD IDE">
        <property
              name="appName"
              value="ABCD IDE">
        </property>
        <property
              name="aboutImage"
              value="platform:/plugin/abcd.xy.metamodel.branding/icons/logo_64.gif">
        </property>
     </product>
  </extension>
</plugin>
