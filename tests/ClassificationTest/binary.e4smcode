model binaryModel{
	package p1 {
		sensor "Sensor Binary" {
			out s1_out
		generates intruderDetected
	},
		binaryClassificationComponent "Binary Classification" {
			in b1_in,
			out b2_out
			confusionMatrixes { confusion matrix cm1 with positive intruderDetected {TP : 200, FN : 2, FP: 5, TN: 8}}
		},
		actuator Actuator {
			in a1_in
		}
		connector c1 "Sensor Binary.s1_out" -> "Binary Classification.b1_in",
		connector c2 "Binary Classification.b2_out" -> "Actuator.a1_in"
	}
	cl intruderDetected, cl c2
		environment Env1 {
		intruderDetected 0.02, not intruderDetected 0.98
	}
}